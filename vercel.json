{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        { "source": "/api/highscores", "destination": "/api/highscores.js" }
    ]
} 